.events-hero-wrapper {
  margin: 0 auto;
  width: 100%;
  height: 80vh;
  background-color: lightblue;
  position: relative;
  overflow: hidden;
}

.events-hero-wrapper:after {
  content: "";
  position: absolute;
  bottom: 0;
  right: -5%;
  left: -5%;
  height: 320px;
  background: #fff;
  -webkit-clip-path: ellipse(50% 60% at 50% 100%);
  clip-path: ellipse(50% 60% at 50% 100%);
}

.refill-curve{
  clip-path: polygon(0 0, 100% 0, 87% 100%, 0% 100%);
}

@media only screen and (max-width: 768px) {
  .testimonial-row{
    min-height: auto;
    height: 100%;
  }

  .events-hero-wrapper{
    height: 30vh;
  } 
  .events-hero-wrapper:after {
    content: "";
    height: 120px;
  }

  .refill-curve{
    clip-path: polygon(0 13%, 100% 0, 100% 100%, 0% 100%);
  }
}

@media only screen and (max-width: 508px) {
  .events-hero-wrapper:after {
    content: "";
    height: 60px;
  }
}

